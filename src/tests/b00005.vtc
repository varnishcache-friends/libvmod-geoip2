varnishtest "Test passing a null ip fails gracefully"

server s1 {
	rxreq
	txresp
} -start

shell "cp ${vmod_topsrc}/maxmind-db/test-data/*.mmdb ${tmpdir}"

varnish v1 -vcl+backend {
	import geoip2 from "${vmod_topbuild}/src/.libs/libvmod_geoip2.so";
	import std;
	import vtc;

	sub vcl_init {
		new db = geoip2.geoip2("${tmpdir}/GeoIP2-City-Test.mmdb");
		std.log(db.lookup("city/names/en", vtc.no_ip()));
	}
} -start

logexpect l1 -v v1 -g raw -d 1 {
	expect * 0	Error \
		"geoip2.lookup: Missing ip address"
} -start

client c1 {
	txreq
	rxresp
} -run

logexpect l1 -wait
